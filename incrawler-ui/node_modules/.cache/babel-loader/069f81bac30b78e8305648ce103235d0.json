        <v-text-field\r\n              v-model=\"search\"\r\n              append-icon=\"mdi-magnify\"\r\n              hide-details\r\n              label=\"Search\"\r\n              single-line\r\n          ></v-text-field>\r\n          <v-spacer></v-spacer>\r\n          <v-dialog v-model=\"dialogDelete\" max-width=\"520px\">\r\n            <v-card>\r\n              <v-card-title class=\"headline\">Are you sure you want to delete this profile?</v-card-title>\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"blue darken-1\" text @click=\"closeDelete\">Cancel</v-btn>\r\n                <v-btn color=\"blue darken-1\" text @click=\"deleteItemConfirm\">OK</v-btn>\r\n                <v-spacer></v-spacer>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n          <v-dialog v-model=\"dialogRecrawling\" max-width=\"640px\">\r\n            <v-card>\r\n              <v-card-title class=\"headline\">Are you sure you want to trigger recrawling this profile?</v-card-title>\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"blue darken-1\" text @click=\"closeRecrawling\">Cancel</v-btn>\r\n                <v-btn color=\"blue darken-1\" text @click=\"recrawlingConfirm\">OK</v-btn>\r\n                <v-spacer></v-spacer>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n          <v-dialog v-model=\"dialogProcess\" max-width=\"600px\">\r\n            <v-card>\r\n              <v-card-title class=\"headline\">Are you sure you want to process this profile?</v-card-title>\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"blue darken-1\" text @click=\"closeProcess\">Cancel</v-btn>\r\n                <v-btn color=\"blue darken-1\" text @click=\"processProfileConfirm\">OK</v-btn>\r\n                <v-spacer></v-spacer>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n        </v-toolbar>\r\n      </template>\r\n      <template v-slot:item.actions=\"{ item }\">\r\n        <v-tooltip bottom>\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-icon\r\n                :v-if=\"hasAdminAuthority\"\r\n                v-bind=\"attrs\"\r\n                v-on=\"on\"\r\n                class=\"mr-2\"\r\n                small\r\n                @click=\"recrawlProfile(item)\"\r\n            >\r\n              mdi-restart\r\n            </v-icon>\r\n          </template>\r\n          <span>Re-Crawl</span>\r\n        </v-tooltip>\r\n        <v-tooltip bottom>\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-icon\r\n                :v-if=\"hasAdminAuthority\"\r\n                v-bind=\"attrs\"\r\n                v-on=\"on\"\r\n                class=\"mr-2\"\r\n                small\r\n                @click=\"processProfile(item)\"\r\n            >\r\n              mdi-account-convert\r\n            </v-icon>\r\n          </template>\r\n          <span>Process</span>\r\n        </v-tooltip>\r\n        <v-tooltip bottom>\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-icon\r\n                :v-if=\"hasAdminAuthority\"\r\n                v-bind=\"attrs\"\r\n                v-on=\"on\"\r\n                small\r\n                @click=\"deleteItem(item)\"\r\n            >\r\n              mdi-delete\r\n            </v-icon>\r\n          </template>\r\n          <span>Delete</span>\r\n        </v-tooltip>\r\n      </template>\r\n      <template v-slot:no-data>\r\n        <v-card-text>\r\n          No Crawled Profile\r\n        </v-card-text>\r\n      </template>\r\n    </v-data-table>\r\n    <v-snackbar\r\n        v-model=\"snackbar\"\r\n        :timeout=\"snackbarTimeout\"\r\n    >\r\n      {{ snackbarText }}\r\n\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn\r\n            v-bind=\"attrs\"\r\n            color=\"blue\"\r\n            text\r\n            @click=\"snackbar = false\"\r\n        >\r\n          Close\r\n        </v-btn>\r\n      </template>\r\n    </v-snackbar>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport Vue from \"vue\";\r\nimport {axiosInstance} from \"@/utils/http-common\";\r\nimport moment from 'moment'\r\nimport {getInstance} from \"@/utils/KeycloakUtil\";\r\nimport { EventBus } from \"@/utils/event-bus\";\r\n\r\nconst keycloak = getInstance();\r\n\r\nexport default {\r\n  components: {},\r\n  data: () => ({\r\n    search: \"\",\r\n    hasAdminAuthority: false,\r\n    hasUserAuthority: false,\r\n    snackbar: false,\r\n    snackbarText: '',\r\n    snackbarTimeout: 5000,\r\n    options: {},\r\n    serverItemsLength: 0,\r\n    dialog: false,\r\n    dialogDelete: false,\r\n    dialogProcess: false,\r\n    dialogRecrawling: false,\r\n    headers: [\r\n      {text: 'Full Name', value: 'username', sortable: true},\r\n      {text: 'Last Crawl Date', align: 'start', value: 'lastCrawlDate', sortable: true},\r\n      {text: 'Is Processed', value: 'isProcessed', sortable: true},\r\n      {text: 'Last Process Date', value: 'lastProcessDate', sortable: true},\r\n      {text: 'Actions', value: 'actions', sortable: false},\r\n    ],\r\n    crawledProfiles: [],\r\n    selectedCrawledProfile: {\r\n      username: \"\",\r\n      lastCrawlDate: null,\r\n      isProcessed: \"\",\r\n      lastProcessDate: null,\r\n      referenceKey: \"\"\r\n    },\r\n    defaultItem: {\r\n      username: \"\",\r\n      lastCrawlDate: null,\r\n      isProcessed: \"\",\r\n      lastProcessDate: null,\r\n      referenceKey: \"\"\r\n    },\r\n    loading: false,\r\n  }),\r\n\r\n  watch: {\r\n    dialog(val) {\r\n      val || this.close()\r\n    },\r\n    dialogDelete(val) {\r\n      val || this.closeDelete()\r\n    },\r\n    dialogRecrawling(val) {\r\n      val || this.closeRecrawling()\r\n    },\r\n    dialogProcess(val) {\r\n      val || this.closeProcess()\r\n    },\r\n    options: {\r\n      handler() {\r\n        this.initialize()\r\n      },\r\n      deep: true,\r\n    },\r\n    search: {\r\n      handler() {\r\n        this.initialize()\r\n      },\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    EventBus.$on(\"onAuthStatusUpdate\", (status) => {\r\n      this.updateAuthStatus();\r\n    });\r\n    this.updateAuthStatus();\r\n  },\r\n\r\n  methods: {\r\n    updateAuthStatus(){\r\n      this.hasAdminAuthority = keycloak.hasRole(\"ADMIN\");\r\n      this.hasUserAuthority = keycloak.hasRole(\"USER\");\r\n    },\r\n    initialize() {\r\n      const {sortBy, sortDesc, page, itemsPerPage} = this.options\r\n      let sortText = \"\";\r\n      if (sortBy.length > 0) {\r\n        sortText = sortBy[0];\r\n        if (sortText.includes(\".text\")) {\r\n          sortText = sortText.replace(\".text\", \"\");\r\n        } else if (sortText == \"isProcessed\") {//bu alan son process tarihine gore dolduruldugu icin bu alana gore siralanacak\r\n          sortText = \"lastProcessDate\";\r\n        }\r\n      }\r\n      axiosInstance.get(`get-all-crawled-profiles?` + \"&search=\" + this.search + \"&page=\" + page + \"&size=\" + itemsPerPage + \"&sortBy=\" + sortText + \"&sortDesc=\" + sortDesc)\r\n          .then(response => {\r\n            if (response.status == 200) {\r\n              this.crawledProfiles = [];\r\n              this.serverItemsLength = response.data.totalItemCount;\r\n              this.options.page = response.data.page + 1;\r\n              this.options.itemsPerPage = response.data.size;\r\n\r\n              response.data.items.forEach(function (item, index) {\r\n                const resItem = {\r\n                  username: item.username,\r\n                  lastCrawlDate: this.format_date(item.lastCrawlDate),\r\n                  lastProcessDate: this.format_date(item.lastProcessDate),\r\n                  referenceKey: item.referenceKey,\r\n                  isProcessed: item.lastProcessDate != null ? \"Processed\" : \"Not Processed\"\r\n                }\r\n                this.crawledProfiles.push(resItem)\r\n              }, this);\r\n            }\r\n          })\r\n          .catch(e => {\r\n            Vue.$log.error(e)\r\n          })\r\n\r\n    },\r\n\r\n    recrawlProfile(item) {\r\n      this.selectedCrawledProfile = Object.assign({}, item)\r\n      this.dialogRecrawling = true\r\n    },\r\n\r\n    processProfile(item) {\r\n      this.selectedCrawledProfile = Object.assign({}, item)\r\n      this.dialogProcess = true\r\n    },\r\n\r\n    deleteItem(item) {\r\n      this.selectedCrawledProfile = Object.assign({}, item)\r\n      this.dialogDelete = true\r\n    },\r\n\r\n    deleteItemConfirm() {\r\n      this.showPopup(\"Crawled profile will be deleted...\");\r\n      axiosInstance.delete(\"delete-crawled-profile/\" + this.selectedCrawledProfile.referenceKey)\r\n          .then(response => {\r\n            if (response.status == 200) {\r\n\r\n              const index = this.crawledProfiles.findIndex(value => value.referenceKey == response.data);\r\n              Vue.$log.info(\"ref : \" + response.data + \" index : \" + index)\r\n              if (index > -1) {\r\n                this.crawledProfiles.splice(index, 1);\r\n                this.serverItemsLength -= 1;\r\n                this.showPopup(\"Crawled profile  deleted\");\r\n\r\n                Vue.$log.debug(\"Crawled profile deleted : \" + response.data);\r\n              }\r\n            }\r\n            this.loading = false;\r\n          })\r\n          .catch(e => {\r\n            this.showPopup(\"Could not deleted crawled profile...\");\r\n            Vue.$log.error(e);\r\n            this.loading = false;\r\n          })\r\n\r\n\r\n      this.closeDelete()\r\n    },\r\n\r\n    recrawlingConfirm() {\r\n      this.showPopup(\"Profile recrawling will be started...\");\r\n\r\n      const data = {\r\n        referenceKey: this.selectedCrawledProfile.referenceKey\r\n      }\r\n      axiosInstance.post(\"recrawl-profile/\", data)\r\n          .then(response => {\r\n            if (response.status == 200) {\r\n              const index = this.crawledProfiles.findIndex(value => value.referenceKey == response.data.referenceKey);\r\n              Vue.$log.debug(\"ref : \" + response.data + \" index : \" + index)\r\n              if (index > -1) {\r\n                this.showPopup(\"Profile recrawling started...\");\r\n                Vue.$log.debug(\"Profile recrawling started : \" + response.data);\r\n              }\r\n            }\r\n            this.loading = false;\r\n          })\r\n          .catch(e => {\r\n            this.showPopup(\"Profile recrawling failed!\")\r\n            Vue.$log.error(e);\r\n            this.loading = false;\r\n          })\r\n\r\n\r\n      this.closeRecrawling();\r\n    },\r\n\r\n    processProfileConfirm() {\r\n      this.showPopup(\"Profile processing will be started...\");\r\n      const data = {\r\n        referenceKey: this.selectedCrawledProfile.referenceKey\r\n      }\r\n      axiosInstance.post(\"process-crawled-profile/\", data)\r\n          .then(response => {\r\n            if (response.status == 200) {\r\n\r\n              const index = this.crawledProfiles.findIndex(value => value.referenceKey == response.data.referenceKey);\r\n              Vue.$log.info(\"ref : \" + response.data + \" index : \" + index)\r\n              if (index > -1) {\r\n                const resItem = {\r\n                  username: response.data.username,\r\n                  lastCrawlDate: this.format_date(response.data.lastCrawlDate),\r\n                  lastProcessDate: this.format_date(response.data.lastProcessDate),\r\n                  referenceKey: response.data.referenceKey,\r\n                  isProcessed: response.data.lastProcessDate != null ? \"Processed\" : \"Not Processed\"\r\n                }\r\n                Object.assign(this.crawledProfiles[index], resItem)\r\n                this.showPopup(\"Profile processing completed\");\r\n\r\n                Vue.$log.debug(\"Profile processing completed : \" + response.data);\r\n              }\r\n            }\r\n            this.loading = false;\r\n          })\r\n          .catch(e => {\r\n            this.showPopup(\"Profile processing failed!\")\r\n            Vue.$log.error(e);\r\n            this.loading = false;\r\n          })\r\n\r\n\r\n      this.closeProcess()\r\n    },\r\n\r\n    close() {\r\n      this.dialog = false\r\n      this.$nextTick(() => {\r\n        this.selectedCrawledProfile = Object.assign({}, this.defaultItem)\r\n      })\r\n      this.$refs.observer.reset();\r\n    },\r\n\r\n    closeDelete() {\r\n      this.dialogDelete = false\r\n      this.$nextTick(() => {\r\n        this.selectedCrawledProfile = Object.assign({}, this.defaultItem)\r\n      })\r\n    },\r\n\r\n    closeRecrawling() {\r\n      this.dialogRecrawling = false\r\n      this.$nextTick(() => {\r\n        this.selectedCrawledProfile = Object.assign({}, this.defaultItem)\r\n      })\r\n    },\r\n\r\n    closeProcess() {\r\n      this.dialogProcess = false\r\n      this.$nextTick(() => {\r\n        this.selectedCrawledProfile = Object.assign({}, this.defaultItem)\r\n      })\r\n    },\r\n\r\n    showPopup(text) {\r\n      this.snackbarText = text\r\n      this.snackbar = true;\r\n    },\r\n\r\n    format_date(value) {\r\n      if (value) {\r\n        return moment(String(value)).format('DD.MM.YYYY hh:mm')\r\n      }\r\n    },\r\n  },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/components/management"}]}